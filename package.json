	},
	"dependencies": {
		"lodash": "4.x",
		"prelude-ls": "1.x",
		"fs.extra": "1.2.x",

		"oop": "git+https://github.com/ymeine/oop#version/latest",

		"http-server": "git+https://github.com/Kykstudio/http-server#version/latest",
		"winston": "0.7.x",

		"pegjs": "0.8.x",
		"escape-html": "1.0.x",
		"ariatemplates": "1.5.x"
